{
    "resource_cd": "ceh.{{ ctx.schema }}.{{ ctx.name }}",
    "resource_desc": "Таблица {{ ctx.schema }}.{{ ctx.name }}",
    "tags": [
{%- if tags|length > 0 %}
{%- for tag in tags %}
        {{tag}}{%- if not loop.last -%},{% endif -%}
{%- endfor %}
{%- endif %}
    ],
    "features": {},
    "datasets": [
        {
            "name": "{{ ctx.name }}",
            "schema_name": "{{ ctx.schema }}"
        }
    ],
    "is_readonly": false
}